{
  "schemaVersion": 39,
  "title": "Execution & OMS Overview",
  "version": 2,
  "editable": true,
  "timezone": "browser",
  "refresh": "30s",
  "panels": [
    {
      "type": "stat",
      "title": "Execution Readiness State",
      "id": 1,
      "gridPos": {"x": 0, "y": 0, "w": 6, "h": 4},
      "options": {
        "reduceOptions": {"values": false, "calcs": ["last"], "fields": ""},
        "orientation": "horizontal",
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center"
      },
      "targets": [
        {
          "expr": "service_readiness_state{service=\"execution\"}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {"type": "value", "options": {"0": {"text": "initializing"}, "1": {"text": "ready"}, "2": {"text": "degraded"}}},
            {"type": "special", "options": {"match": "null", "result": {"text": "n/a"}}}
          ],
          "thresholds": {"mode": "absolute", "steps": [
            {"color": "yellow", "value": 0},
            {"color": "green", "value": 1},
            {"color": "red", "value": 2}
          ]}
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "OMS Initialization Attempts",
      "id": 2,
      "gridPos": {"x": 6, "y": 0, "w": 9, "h": 6},
      "targets": [
        {"expr": "increase(oms_init_attempts_total[15m])", "refId": "A"},
        {"expr": "oms_init_attempts_total", "refId": "B"}
      ],
      "fieldConfig": {"defaults": {"unit": "short"}}
    },
    {
      "type": "table",
      "title": "Execution Components Snapshot",
      "id": 3,
      "gridPos": {"x": 0, "y": 4, "w": 6, "h": 8},
      "targets": [
        {"expr": "service_readiness_state{service=\"execution\"}", "refId": "A"}
      ],
      "transformations": []
    },
    {
      "type": "timeseries",
      "title": "Orders Submitted / Rejected",
      "id": 4,
      "gridPos": {"x": 15, "y": 0, "w": 9, "h": 6},
      "targets": [
        {"expr": "rate(orders_submitted_total[5m])", "refId": "A"},
        {"expr": "rate(orders_rejected_total[5m])", "refId": "B"}
      ],
      "fieldConfig": {"defaults": {"unit": "ops"}}
    },
    {
      "type": "timeseries",
      "title": "Backfill Status Gauge (State Series)",
      "id": 5,
      "gridPos": {"x": 0, "y": 12, "w": 12, "h": 6},
      "targets": [
        {"expr": "historical_backfill_status", "refId": "A"}
      ],
      "fieldConfig": {"defaults": {"unit": "short"}}
    },
    {
      "type": "timeseries",
      "title": "Provider Request Volume (Success vs Error)",
      "id": 6,
      "gridPos": {"x": 12, "y": 12, "w": 12, "h": 6},
      "targets": [
        {"expr": "sum by (provider,status) (rate(provider_requests_total[5m]))", "refId": "A"}
      ],
      "fieldConfig": {"defaults": {"unit": "ops"}}
    },
    {
      "type": "timeseries",
      "title": "Provider Attempts Rate",
      "id": 7,
      "gridPos": {"x": 0, "y": 18, "w": 12, "h": 6},
      "targets": [
        {"expr": "sum by (provider,endpoint) (rate(provider_attempts_total[5m]))", "refId": "A"}
      ],
      "fieldConfig": {"defaults": {"unit": "ops"}}
    },
    {
      "type": "timeseries",
      "title": "Provider Latency p95 (s)",
      "id": 8,
      "gridPos": {"x": 12, "y": 18, "w": 12, "h": 6},
      "targets": [
        {"expr": "histogram_quantile(0.95, sum by (le,provider,endpoint) (rate(provider_request_latency_seconds_bucket[5m])))", "refId": "A"}
      ],
      "fieldConfig": {"defaults": {"unit": "s"}}
    }
  ],
  "templating": {"list": []}
}
