{
  "dashboard": {
    "id": null,
    "title": "AI Trading System - Admin Control Panel",
    "tags": ["admin", "control", "management"],
    "style": "dark",
    "timezone": "America/New_York",
    "panels": [
      {
        "id": 1,
        "title": "System Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "up",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Data Source Health",
        "type": "table",
        "targets": [
          {
            "expr": "trading_data_source_status",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4}
      },
      {
        "id": 3,
        "title": "API Key Management",
        "type": "table",
        "targets": [
          {
            "expr": "trading_api_key_status",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4}
      },
      {
        "id": 4,
        "title": "Trading Engine Status",
        "type": "stat",
        "targets": [
          {
            "expr": "trading_engine_active",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 5,
        "title": "Risk Management Status",
        "type": "stat",
        "targets": [
          {
            "expr": "risk_engine_status",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 6,
        "title": "Emergency Controls",
        "type": "button-panel",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
        "options": {
          "buttons": [
            {
              "text": "Emergency Stop",
              "variant": "destructive",
              "onclick": "emergency_stop()"
            },
            {
              "text": "Pause Trading",
              "variant": "secondary",
              "onclick": "pause_trading()"
            }
          ]
        }
      },
      {
        "id": 7,
        "title": "System Configuration",
        "type": "table",
        "targets": [
          {
            "expr": "trading_config_values",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 12}
      },
      {
        "id": 8,
        "title": "Recent System Events",
        "type": "logs",
        "targets": [
          {
            "expr": "{job=\"trading-system\"} |= \"ADMIN\" or \"ERROR\" or \"WARN\"",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 20}
      },
      {
        "id": 9,
        "title": "Resource Utilization",
        "type": "timeseries",
        "targets": [
          {
            "expr": "100 - (avg(rate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
            "legendFormat": "CPU Usage %",
            "refId": "A"
          },
          {
            "expr": "(1 - (node_memory_MemAvailable_bytes / node_memory_MemTotal_bytes)) * 100",
            "legendFormat": "Memory Usage %",
            "refId": "B"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 28}
      },
      {
        "id": 10,
        "title": "Active Connections",
        "type": "timeseries",
        "targets": [
          {
            "expr": "trading_active_connections",
            "legendFormat": "{{source}}",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 28}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s",
    "schemaVersion": 30,
    "version": 1
  }
}